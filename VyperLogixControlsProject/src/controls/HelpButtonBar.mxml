<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			[Embed(source="assets/icons/help.gif")]
			[Bindable]
			public var helpCls:Class;
		]]>
	</fx:Script>
	<mx:VBox width="100%" height="100%">
		<mx:HBox width="100%" horizontalAlign="center" verticalAlign="middle" paddingTop="3">
			<mx:Button id="btn_Help" label="Get Help" labelPlacement="left" icon="{this.helpCls}"/>
		</mx:HBox>
	</mx:VBox>
</mx:Canvas>
