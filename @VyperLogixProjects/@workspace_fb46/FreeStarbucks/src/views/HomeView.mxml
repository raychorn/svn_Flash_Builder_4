<?xml version="1.0" encoding="utf-8"?>
<s:View 
	title="Get YOUR Free Starbucks !!!"
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:mx="library://ns.adobe.com/flex/mx"
	creationComplete="view1_creationCompleteHandler(event)" 
	contentBackgroundColor="#ffffff" xmlns:views="views.*">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import com.DebuggerUtils;
			
			import controls.Alert.AlertAlternative;
			
			import free4u.ApplicationProxy;
			
			import mx.core.FlexGlobals;
			import mx.events.FlexEvent;
			
			protected function view1_creationCompleteHandler(event:FlexEvent):void {
				if (FlexGlobals.topLevelApplication.set_navigator_using is Function) {
					FlexGlobals.topLevelApplication.set_navigator_using(this.navigator);
				} else {
					AlertAlternative.error('WARNING: <p>Framework Error in "'+DebuggerUtils.getFunctionName(new Error())+'"</p>');
				}
			}
			
			public function handle_checkin_event():void {
				this.navigator.pushView(DonationView,{});
			}
		]]>
	</fx:Script>
	<views:HomeViewControl id="homeView" width="100%" height="100%"/>
</s:View>
