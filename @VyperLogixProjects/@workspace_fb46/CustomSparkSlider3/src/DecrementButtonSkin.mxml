<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009" alpha.disabled="0.5" minWidth="9" minHeight="9" >
    
    <fx:Metadata>
        <![CDATA[ 
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>
    
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
    <s:Rect id="border" left="0" right="0" top="0" bottom="0">
        <s:stroke>
            <s:SolidColorStroke color="0x000000" />
        </s:stroke>
        <s:fill>
            <s:SolidColor color="0xe3e3e3" color.over="0xFFFFFF" />
        </s:fill>
    </s:Rect>
    
    <s:Rect left="2" right="2" verticalCenter="0" height="1">
        <s:fill>
            <s:SolidColor color="0x000000" />
        </s:fill>
    </s:Rect>
    
</s:SparkSkin>
