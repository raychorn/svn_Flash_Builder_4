<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" 
			   width="100%" height="100%">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			
			protected function fileExplorerContainer_creationCompleteHandler(event:FlexEvent):void {
				var canvas:HGroup = event.currentTarget as HGroup;
			}
			
		]]>
	</fx:Script>
	<s:VGroup width="100%" height="100%">
		<s:HGroup id="fileExplorerContainer" width="100%" creationComplete="fileExplorerContainer_creationCompleteHandler(event)">
		</s:HGroup>
	</s:VGroup>
</s:TitleWindow>
