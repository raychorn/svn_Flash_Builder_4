<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="{data.firstName} {data.lastName}" >
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import events.ScrollingEvent;
			
			import spark.events.ViewNavigatorEvent;
			

		]]>
	</fx:Script>
	<s:Image id="img" source="{data.picture}" width="90"  left="20" top="20"/>
	<s:Label left="{img.width + 40}" top="40" text="{data.title}"/>
	<s:Label left="{img.width + 40}" top="80" text="{data.city}"/>
	<s:Label left="{img.width + 40}" top="120" text="{data.department}"/>
	<s:Label left="{img.width + 40}" top="160" text="{data.officePhone}"/>
	<s:Label left="{img.width + 40}" top="200" text="{data.email}"/>
</s:View>
