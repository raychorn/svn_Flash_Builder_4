<?xml version="1.0" encoding="utf-8"?>
<s:Group width="100%" height="100%" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="http://www.adobe.com/2006/mxml">
	<mx:Script>
		<![CDATA[
			[Bindable]
			public var _buttonIcon:Class;
		]]>
	</mx:Script>
	<s:VGroup width="100%" height="100%" horizontalAlign="left" verticalAlign="top">
		<s:Panel id="panel_NoAccess" styleName="PanelNoAccess" width="100%" height="100%" title="Access Denied">
			<s:VGroup height="100%" width="100%" verticalAlign="middle">
				<s:Label text="Access Denied !" styleName="NoAccessText"/>
				<s:HGroup width="100%" horizontalAlign="center" paddingTop="30">
					<s:Button label="Login" id="btn_login" icon="{this._buttonIcon}" styleName="LargeButton"/>
				</s:HGroup>
			</s:VGroup>
		</s:Panel>
	</s:VGroup>
</s:Group>
