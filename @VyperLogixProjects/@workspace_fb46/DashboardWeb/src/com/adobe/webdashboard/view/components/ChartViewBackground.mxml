		<s:fill>
			<s:SolidColor color="0x1A1A1A"/>
		</s:fill>
	</s:Rect>
	
	<!-- seperator bar -->
	<s:Rect left="37" right="37" height="1" y="42">
		<s:fill>
			<s:SolidColor color="0x212121"/>
		</s:fill>
	</s:Rect>
	<s:Rect left="37" right="37" height="1" y="43">
		<s:fill>
			<s:SolidColor color="0x000000"/>
		</s:fill>
	</s:Rect>
	<s:Rect left="37" right="37" height="1" y="44">
		<s:fill>
			<s:SolidColor color="0x212121"/>
		</s:fill>
	</s:Rect>
	
	<!-- data content background -->
	<s:Rect left="36" right="36" top="97" bottom="19">
		<s:fill>
			<s:SolidColor color="0x000000"/>
		</s:fill>
		<s:filters>
			<s:GlowFilter color="0x00000" blurX="10" blurY="10" alpha="0.5" />
		</s:filters>
	</s:Rect>
